.products.push(data);\n        cont += 1;\n      }\n      this.loading = true;\n    },\n    async cargarCarrito(id) {\n      debugger;\n      const data = await axios.get(`${API_URL}carts/${id}`);\n      this.yourCart = data;\n    }\n  },\n  watch: {\n    yourCart(oldval, newVal) {}\n  },\n  components: {\n    NavBar\n  }\n};","map":{"version":3,"mappings":";AAoFA,OAAOA,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,OAAO,EAAEC,OAAM,QAAS,6BAA6B;AAC9D;AACA,eAAe;EACbC,IAAI,EAAE,aAAa;EACnB,MAAMC,OAAO,GAAG;IACd,IAAI,CAACC,MAAK,GAAIC,IAAI,CAACC,KAAK,CAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,EAAE,CAAC;IAC9C,MAAM,IAAI,CAACC,aAAa,CAAC,IAAI,CAACN,MAAM,CAAC;IACrC;IAEA,MAAM,IAAI,CAACO,eAAe,CAAC,IAAI,CAACC,QAAQ,CAACC,KAAK,CAAC;IAC/C,IAAI,CAACC,WAAU,GAAI,IAAI;IACvBC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,QAAQ,CAAC;EAC5B,CAAC;EACDC,IAAI,EAAE,OAAO;IACXD,QAAQ,EAAE,EAAE;IACZE,OAAO,EAAE,KAAK;IACdf,MAAM,EAAE,IAAI;IACZQ,QAAQ,EAAE,IAAI;IACdQ,MAAM,EAAEnB,OAAO;IACfa,WAAW,EAAE;EACf,CAAC,CAAC;EACFO,OAAO,EAAE;IACPC,gBAAgB,CAACC,OAAO,EAAE;MACxB,MAAMC,YAAW,GAAI,IAAI,CAACP,QAAQ,CAACQ,SAAS,CAAEC,IAAI,IAAK;QACrD,OAAOA,IAAI,CAACjB,EAAC,KAAMc,OAAO;MAC5B,CAAC,CAAC;MACF,IAAI,CAACN,QAAQ,CAACU,MAAM,CAACH,YAAY,EAAE,CAAC,CAAC;MACrC,IAAI,CAACV,WAAU,GAAI,IAAI;IACzB,CAAC;IACD,MAAMc,UAAU,CAACnB,EAAE,EAAE;MACnB,IAAIoB,QAAO,GAAI,CAAC;MAChB,MAAMX,IAAG,GAAI,MAAMY,KAAK,CAAE,GAAE9B,OAAQ,SAAQS,EAAG,EAAC,CAAC,CAACsB,IAAI,CAAEC,GAAG,IACzDA,GAAG,CAACC,IAAI,EAAC,CACV;MACD,KAAK,IAAIC,IAAI,CAAC,EAAEA,KAAKhB,IAAI,CAACL,KAAK,CAACsB,MAAK,GAAI,CAAC,EAAED,CAAC,EAAE,EAAE;QAC/CL,QAAO,IAAKO,QAAQ,CAAClB,IAAI,CAACL,KAAK,CAACqB,CAAC,CAAC,CAACL,QAAQ,CAAC;MAC9C;MAEA,OAAOA,QAAQ;IACjB,CAAC;IACD,MAAMQ,eAAe,CAACC,MAAM,EAAEC,SAAS,EAAE;MACvC,IAAI,CAACzB,WAAU,GAAI,KAAK;MACxB;MACA,MAAM0B,KAAI,GAAI;QACZF,MAAM;QACNC;MACF,CAAC;MACDxB,OAAO,CAACC,GAAG,CAACwB,KAAK,CAAC;MAClB,MAAMtB,IAAG,GAAI,MAAMnB,KAAI,CACpB0C,KAAK,CAAE,GAAEzC,OAAQ,QAAO,EAAEwC,KAAK,EAC/BT,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACd,IAAI,EACtBwB,KAAK,CAAEC,KAAK,IAAK5B,OAAO,CAACC,GAAG,CAAC2B,KAA