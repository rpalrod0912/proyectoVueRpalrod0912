  while (index < data.length) {\n        if (data[index].nombre.split(\" \").join(\"\").toLowerCase().includes(val.split(\" \").join(\"\").toLowerCase())) {\n          encontrados.push(data[index]);\n        }\n        index += 1;\n      }\n      this.searchsArray = encontrados;\n    }\n  },\n  computed: {\n    authentication: false\n  },\n  watch: {\n    text(newval, oldval) {\n      debugger;\n      this.fetchAllProductsByValue(newval);\n    },\n    authentication(newval, oldval) {\n      debugger;\n      if (this.$store.currenAuth === true) {\n        this.$store.commit(\"setCurrentAuth\", true);\n      }\n    },\n    inmediate: true\n  }\n};","map":{"version":3,"mappings":";AA6HA,SAASA,IAAI,EAAEC,OAAM,QAAS,oCAAoC;AAClE;AACA,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,OAAO,GAAG;IACR;IACA,IAAIC,IAAG,GAAI,IAAI;IACfJ,IAAI,CAACK,kBAAkB,CAAC,gBAAgBC,IAAI,EAAE;MAC5C,IAAIA,IAAG,IAAK,IAAI,EAAE;QAChBF,IAAI,CAACG,KAAI,GAAID,IAAI,CAACC,KAAK;QACvBH,IAAI,CAACI,cAAa,GAAI,IAAI;QAC1BJ,IAAI,CAACK,EAAC,GAAIH,IAAI,CAACI,GAAG;QAClBN,IAAI,CAACO,aAAY,GAAI,MAAMP,IAAI,CAACQ,UAAU,CAACN,IAAI,CAACI,GAAG,CAAC;QACpD;MACF,OAAO;QACLN,IAAI,CAACI,cAAa,GAAI,KAAK;MAC7B;IACF,CAAC,CAAC;EACJ,CAAC;EAEDK,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,YAAY,EAAE,EAAE;MAChBP,cAAc,EAAE,IAAI;MACpBD,KAAK,EAAE,EAAE;MACTI,aAAa,EAAE,CAAC;MAChBF,EAAE,EAAE;IACN,CAAC;EACH,CAAC;EACDO,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAChBjB,IAAI,EAAE,aAAa;QACnBkB,KAAK,EAAE;UAAEX,EAAE,EAAEY,IAAI,CAACC,SAAS,CAAC,IAAI,CAACb,EAAE;QAAE;MACvC,CAAC,CAAC;IACJ,CAAC;IACD,MAAMG,UAAU,CAACH,EAAE,EAAE;MACnB,IAAIc,QAAO,GAAI,CAAC;MAChB,MAAMV,IAAG,GAAI,MAAMW,KAAK,CAAE,sCAAqCf,EAAG,EAAC,CAAC,CAACgB,IAAI,CACtEC,GAAG,IAAKA,GAAG,CAACC,IAAI,EAAC,CACnB;MACD,KAAK,IAAIC,IAAI,CAAC,EAAEA,KAAKf,IAAI,CAACgB,KAAK,CAACC,MAAK,GAAI,CAAC,EAAEF,CAAC,EAAE,EAAE;QAC/CL,QAAO,IAAKQ,QAAQ,CAAClB,IAAI,CAACgB,KAAK,CAACD,CAAC,CAAC,CAACL,QAAQ,CAAC;MAC9C;MACA,OAAOA,QAAQ;IACjB,CAAC;IACDS,MAAM,GAAG;MACP/B,OAAO,CAACD,IAAI,EACTyB,IAAI,CAAC,MAAM;QACV,IAAI,CAACjB,cA