{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { auth, signOut } from \"@/firebaseConfig/firebaseConfig.js\";\n/*eslint-disable */\nexport default {\n  name: \"NavBar\",\n  created() {\n    //OBLIGATORIO DE LO CONTRARIO NO PODEMOS TRABAJAR CON VARIABLESDFE VUE CON oAuthStateChanged\n    let self = this;\n    console.log(auth.currentUser);\n    auth.onAuthStateChanged(async function (user) {\n      if (user != null) {\n        console.log(\"ESTAS LOGEADO\");\n        self.email = user.email;\n        self.authentication = true;\n        debugger;\n        self.carritoNumero = await self.contarProd(user.uid);\n      } else {\n        console.log(\"NO ESTAS LOGEADO\");\n        self.authentication = false;\n      }\n    });\n    console.log(this.text);\n  },\n  data() {\n    return {\n      text: \"\",\n      searchsArray: [],\n      authentication: null,\n      email: \"\",\n      carritoNumero: 0\n    };\n  },\n  methods: {\n    async contarProd(id) {\n      let cantidad = 0;\n      const data = await fetch(`http://localhost:3003/v1/api/carts/${id}`).then(res => res.json());\n      for (let i = 0; i <= data.cesta.length - 1; i++) {\n        cantidad += parseInt(data.cesta[i].cantidad);\n      }\n      return cantidad;\n    },\n    logOut() {\n      debugger;\n      signOut(auth).then(() => {\n        console.log(\"Desconectado\");\n        this.authentication = false;\n        debugger;\n        this.$router.go(\"/\");\n      }).catch(error => {\n        console.log(\"ALGO OCURRIO\");\n      });\n    },\n    buscarElementos() {\n      this.$router.push({\n        name: \"BusquedasView\",\n        query: {\n          prodFiltrados: JSON.stringify(this.searchsArray)\n        }\n      });\n    },\n    async fetchAllProductsByValue(val) {\n      const data = await fetch(`http://localhost:3003/v1/api/productos`).then(res => res.json());\n      let index = 0;\n      let encontrados = [];\n      while (index < data.length) {\n        if (data[index].nombre.split(\" \").join(\"\").toLowerCase().includes(val.split(\" \").join(\"\").toLowerCase())) {\n          encontrados.push(data[index]);\n        }\n        index += 1;\n      }\n      this.searchsArray = encontrados;\n    }\n  },\n  computed: {\n    authentication: false\n  },\n  watch: {\n    text(newval, oldval) {\n      debugger;\n      this.fetchAllProductsByValue(newval);\n    },\n    authentication(newval, oldval) {},\n    inmediate: true\n  }\n};","map":{"version":3,"mappings":";AA0GA,SAASA,IAAI,EAAEC,OAAM,QAAS,oCAAoC;AAClE;AACA,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,OAAO,GAAG;IACR;IACA,IAAIC,IAAG,GAAI,IAAI;IACfC,OAAO,CAACC,GAAG,CAACN,IAAI,CAACO,WAAW,CAAC;IAC7BP,IAAI,CAACQ,kBAAkB,CAAC,gBAAgBC,IAAI,EAAE;MAC5C,IAAIA,IAAG,IAAK,IAAI,EAAE;QAChBJ,OAAO,CAACC,GAAG,CAAC,eAAe,CAAC;QAC5BF,IAAI,CAACM,KAAI,GAAID,IAAI,CAACC,KAAK;QACvBN,IAAI,CAACO,cAAa,GAAI,IAAI;QAC1B;QACAP,IAAI,CAACQ,aAAY,GAAI,MAAMR,IAAI,CAACS,UAAU,CAACJ,IAAI,CAACK,GAAG,CAAC;MACtD,OAAO;QACLT,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;QAC/BF,IAAI,CAACO,cAAa,GAAI,KAAK;MAC7B;IACF,CAAC,CAAC;IACFN,OAAO,CAACC,GAAG,CAAC,IAAI,CAACS,IAAI,CAAC;EACxB,CAAC;EAEDC,IAAI,GAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MACRE,YAAY,EAAE,EAAE;MAChBN,cAAc,EAAE,IAAI;MACpBD,KAAK,EAAE,EAAE;MACTE,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDM,OAAO,EAAE;IACP,MAAML,UAAU,CAACM,EAAE,EAAE;MACnB,IAAIC,QAAO,GAAI,CAAC;MAChB,MAAMJ,IAAG,GAAI,MAAMK,KAAK,CAAE,sCAAqCF,EAAG,EAAC,CAAC,CAACG,IAAI,CACtEC,GAAG,IAAKA,GAAG,CAACC,IAAI,EAAC,CACnB;MACD,KAAK,IAAIC,IAAI,CAAC,EAAEA,KAAKT,IAAI,CAACU,KAAK,CAACC,MAAK,GAAI,CAAC,EAAEF,CAAC,EAAE,EAAE;QAC/CL,QAAO,IAAKQ,QAAQ,CAACZ,IAAI,CAACU,KAAK,CAACD,CAAC,CAAC,CAACL,QAAQ,CAAC;MAC9C;MACA,OAAOA,QAAQ;IACjB,CAAC;IACDS,MAAM,GAAG;MACP;MACA5B,OAAO,CAACD,IAAI,EACTsB,IAAI,CAAC,MAAM;QACVjB,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;QAC3B,IAAI,CAACK,cAAa,GAAI,KAAK;QAC3B;QACA,IAAI,CAACmB,OAAO,CAACC,EAAE,CAAC,GAAG,CAAC;MACtB,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChB5B,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B,CAAC,CAAC;IACN,CAAC;IACD4B,eAAe,GAAG;MAChB,IAAI,CAACJ,OAAO,CAACK,IA