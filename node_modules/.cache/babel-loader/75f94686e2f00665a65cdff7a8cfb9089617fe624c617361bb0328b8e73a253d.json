tile>\n      </div>\n    </v-list>\n\n    <v-container>\n      <v-btn color=\"success\" larger style=\"float: right\">Go to payment</v-btn>\n    </v-container>\n  </v-container>\n  <h2 v-else>Cargando...</h2>\n</template>\n\n<script>\nimport NavBar from \"@/components/NavBar.vue\";\n/*eslint-disable */\nexport default {\n  name: \"CarritoView\",\n  async created() {\n    debugger;\n\n    this.cartId = JSON.parse(this.$route.query.id);\n    await this.cargarCarrito(this.cartId);\n    await this.cargarProductos(this.yourCart.cesta);\n  },\n  data: () => ({\n    products: [],\n    loading: false,\n    cartId: null,\n    yourCart: null,\n  }),\n  methods: {\n    async cargarProducto(cesta) {\n      let cont = 0;\n      while (cont <= cesta.length) {\n        const data = await fetch(\n          `http://localhost:3003/v1/api/productos/${this.yourCart.cesta[cont].idProducto}`\n        ).then((res) => res.json());\n        this.products.push(data);\n      }\n    },\n    async cargarCarrito(id) {\n      debugger;\n      const data = await fetch(`http://localhost:3003/v1/api/carts/${id}`).then(\n        (res) => res.json()\n      );\n      this.yourCart = data;\n    },\n  },\n  watch: {\n    yourCart(oldval, newVal) {\n      this.loading = true;\n    },\n  },\n  components: { NavBar },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}