ue : false;\n    },\n    validateLastName: () => this.userData.userLastName.length <= 15 ? true : false,\n    validateMail: () => /^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$/.test(this.userData.userMail) ? true : false,\n    validatePwd() {\n      if (/^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#$%]).{8,24}$/.test(this.userData.userPwd) && this.registerConfirmPwd === this.userData.userPwd) return true;\n    }\n  },\n  watch: {\n    userData: {\n      handler(newVal, oldVal) {\n        this.validateAll();\n      },\n      deep: true\n    }\n  }\n};","map":{"version":3,"mappings":"AA6FA;AACA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,QAAQ,EAAE,QAAQ;QAClBC,YAAY,EAAE,oBAAoB;QAClCC,QAAQ,EAAE,oBAAoB;QAC9BC,OAAO,EAAE;MACX,CAAC;MACDC,IAAI,EAAE,MAAM;MACZC,QAAQ,EAAE,KAAK;MACfC,KAAK,EAAE,IAAI;MACX;MACA;MACAC,kBAAkB,EAAE,EAAE;MACtBC,eAAe,EAAE,KAAK;MACtBC,gBAAgB,EAAE,CACfC,GAAG,IAAK,CAAC,CAACA,GAAE,IAAK,0BAA0B,EAC3CA,GAAG,IACDA,GAAE,IAAKA,GAAG,CAACC,MAAK,IAAK,EAAE,IACxB,0CAA0C,CAC7C;MACDC,oBAAoB,EAAE,CACnBF,GAAG,IACF,yDAAyD,CAACN,IAAI,CAACM,GAAG,KAClE,kGAAkG,CACrG;MACDG,cAAc,EAAE,CACbH,GAAG,IACF,kCAAkC,CAACN,IAAI,CAACM,GAAG,KAC3C,2EAA2E,CAC9E;MACDI,uBAAuB,EAAE,CACtBJ,GAAG,IACF,yDAAyD,CAACN,IAAI,CAACM,GAAG,KAClE,kGAAkG,CACrG;MACDK,eAAe,EAAE,CACdC,CAAC,IAAK,CAAC,CAACA,KAAK,uDAAuD;IAEzE,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQ,GAAG;MACTC,OAAO,CAACC,GAAG,CAAC,MAAM,CAAC;MACnB,IAAI,CAACC,KAAK,CAACC,IAAI,CAACC,QAAQ,EAAE;IAC5B;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,wBAAwB,GAAG;MACzB,OAAO,MACL,IAAI,CAAC1B,QAAQ,CAACI,OAAM,KAAM,IAAI,CAACI,kBAAiB,IAChD,iCAAiC;IACrC,CAAC;IACDmB,YAAY,GAAG;MACbP,OAAO,CAACC,GAAG,CAAC,IAAI,CAACrB,QAAQ,CAACC,QAAQ,CAAC;MACnC,IAAI,CAACD,QAAQ,CAACC,QAAQ,CAACW,MAAK,IAAK,EAAC,GAAI,IAAG,GAAI,KAAK;IACpD;EACF,CAAC;EACDM,OAAO,EAAE;IACPU,WAAW,GAAG;MACZ,IAAI,IAAI,CAACD,YAAY,EAAC,KAAM,IAAI,EAAE;QAChCP,OAAO,CAACC,GAAG,CAAC,aAAa,CAAC;QAC1B,IAAI,CAACf,QAAO,GAAI,IAAI;MACtB;IACF,CAAC;IACDqB,YAAY,EAAE,MAAM;MAClBP,OAAO,CAACC,GAAG,CAAC,IAAI,CAACrB,QAAQ,CAACC,QAAQ,CAAC;MACnC,IAAI,CAACD