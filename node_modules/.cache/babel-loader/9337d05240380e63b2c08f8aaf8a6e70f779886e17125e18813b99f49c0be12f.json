{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { auth, signOut } from \"@/firebaseConfig/firebaseConfig.js\";\n/*eslint-disable */\nexport default {\n  name: \"NavBar\",\n  created() {\n    debugger;\n    //OBLIGATORIO DE LO CONTRARIO NO PODEMOS TRABAJAR CON VARIABLESDFE VUE CON oAuthStateChanged\n    let self = this;\n    console.log(auth.currentUser);\n    auth.onAuthStateChanged(function (user) {\n      if (user != null) {\n        debugger;\n        console.log(\"ESTAS LOGEADO\");\n        self.email = user.email;\n        self.authentication = true;\n      } else {\n        console.log(\"NO ESTAS LOGEADO\");\n        self.authentication = false;\n      }\n    });\n    console.log(this.text);\n  },\n  data() {\n    return {\n      text: \"\",\n      searchsArray: [],\n      authentication: null,\n      email: \"\"\n    };\n  },\n  methods: {\n    logOut() {\n      signOut(auth).then(() => {\n        console.log(\"Desconectado\");\n        this.authentication = false;\n      }).catch(error => {\n        console.log(\"ALGO OCURRIO\");\n      });\n    },\n    buscarElementos() {\n      this.$router.push({\n        name: \"BusquedasView\",\n        query: {\n          prodFiltrados: JSON.stringify(this.searchsArray)\n        }\n      });\n    },\n    async fetchAllProductsByValue(val) {\n      const data = await fetch(`http://localhost:3003/v1/api/productos`).then(res => res.json());\n      let index = 0;\n      let encontrados = [];\n      while (index < data.length) {\n        if (data[index].nombre.split(\" \").join(\"\").toLowerCase().includes(val.split(\" \").join(\"\").toLowerCase())) {\n          encontrados.push(data[index]);\n        }\n        index += 1;\n      }\n      this.searchsArray = encontrados;\n    }\n  },\n  computed: {\n    authentication: false\n  },\n  watch: {\n    text(newval, oldval) {\n      debugger;\n      this.fetchAllProductsByValue(newval);\n    },\n    authentication(newval, oldval) {},\n    inmediate: true\n  }\n};","map":{"version":3,"mappings":";AAyGA,SAASA,IAAI,EAAEC,OAAM,QAAS,oCAAoC;AAClE;AACA,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,OAAO,GAAG;IACR;IACA;IACA,IAAIC,IAAG,GAAI,IAAI;IACfC,OAAO,CAACC,GAAG,CAACN,IAAI,CAACO,WAAW,CAAC;IAC7BP,IAAI,CAACQ,kBAAkB,CAAC,UAAUC,IAAI,EAAE;MACtC,IAAIA,IAAG,IAAK,IAAI,EAAE;QAChB;QACAJ,OAAO,CAACC,GAAG,CAAC,eAAe,CAAC;QAC5BF,IAAI,CAACM,KAAI,GAAID,IAAI,CAACC,KAAK;QACvBN,IAAI,CAACO,cAAa,GAAI,IAAI;MAC5B,OAAO;QACLN,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;QAC/BF,IAAI,CAACO,cAAa,GAAI,KAAK;MAC7B;IACF,CAAC,CAAC;IACFN,OAAO,CAACC,GAAG,CAAC,IAAI,CAACM,IAAI,CAAC;EACxB,CAAC;EAEDC,IAAI,GAAG;IACL,OAAO;MAAED,IAAI,EAAE,EAAE;MAAEE,YAAY,EAAE,EAAE;MAAEH,cAAc,EAAE,IAAI;MAAED,KAAK,EAAE;IAAG,CAAC;EACxE,CAAC;EACDK,OAAO,EAAE;IACPC,MAAM,GAAG;MACPf,OAAO,CAACD,IAAI,EACTiB,IAAI,CAAC,MAAM;QACVZ,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;QAC3B,IAAI,CAACK,cAAa,GAAI,KAAK;MAC7B,CAAC,EACAO,KAAK,CAAEC,KAAK,IAAK;QAChBd,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B,CAAC,CAAC;IACN,CAAC;IACDc,eAAe,GAAG;MAChB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAChBpB,IAAI,EAAE,eAAe;QACrBqB,KAAK,EAAE;UAAEC,aAAa,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACZ,YAAY;QAAE;MAC5D,CAAC,CAAC;IACJ,CAAC;IACD,MAAMa,uBAAuB,CAACC,GAAG,EAAE;MACjC,MAAMf,IAAG,GAAI,MAAMgB,KAAK,CAAE,wCAAuC,CAAC,CAACZ,IAAI,CACpEa,GAAG,IAAKA,GAAG,CAACC,IAAI,EAAC,CACnB;MACD,IAAIC,KAAI,GAAI,CAAC;MACb,IAAIC,WAAU,GAAI,EAAE;MACpB,OAAOD,KAAI,GAAInB,IAAI,CAACqB,MAAM,EAAE;QAC1B,IACErB,IAAI,CAACmB,KAAK,CAAC,CAACG,MAAK,CACdC,KAAK,CAAC,GAAG,EACTC,IAAI,CAAC,EAAE,EACPC,WAAW,EAAC,CACZC,QAAQ,CAACX,GAAG,CAACQ,KAAK,CAAC,GAAG,CAAC,CAACC,IAAI,CAAC,EAAE,CAAC,CAACC,WAAW,EAAE,GACjD;UACAL,WAAW,CAACX,IAAI,CAACT,IAAI,CAACmB,KAAK,CAAC,CAAC;QAC/B;QACAA,KAAI,IAAK,CAAC;MACZ;MACA,IAAI,CAAClB,YAAW,GAAImB,WAAW;IACjC;EACF,CAAC;EACDO,QAAQ,EAAE;IACR7B,cAAc,EAAE;EAClB,CAAC;EACD8B,KAAK,EAAE;IACL7B,IAAI,CAAC8B,MAAM,EAAEC,MAAM,EAAE;MACnB;MACA,IAAI,CAAChB,uBAAuB,CAACe,MAAM,CAAC;IACtC,CAAC;IACD/B,cAAc,CAAC+B,MAAM,EAAEC,MAAM,EAAE,CAAC,CAAC;IACjCC,SAAS,EAAE;EACb;AACF,CAAC","names":["auth","signOut","name","created","self","console","log","currentUser","onAuthStateChanged","user","email","authentication","text","data","searchsArray","methods","logOut","then","catch","error","buscarElementos","$router","push","query","prodFiltrados","JSON","stringify","fetchAllProductsByValue","val","fetch","res","json","index","encontrados","length","nombre","split","join","toLowerCase","includes","computed","watch","newval","oldval","inmediate"],"sourceRoot":"","sources":["/home/rafa/Vue/proyectovue/src/components/NavBar.vue"],"sourcesContent":["<template>\n  <v-app-bar class=\"header\" app color=\"#3853D8\" dark>\n    <div class=\"titulo\">\n      <v-hover v-slot:default=\"{ isHovering, props }\" open-delay=\"60\">\n        <img\n          v-bind=\"props\"\n          style=\"max-width: 13rem\"\n          src=\"https://i.ibb.co/hLsC5XG/whitelogo.png\"\n          alt=\"whitelogo\"\n        />\n        <img\n          v-if=\"isHovering\"\n          src=\"https://i.ibb.co/dJT1MtP/2.png\"\n          alt=\"2\"\n          border=\"0\"\n        />\n\n        />\n      </v-hover>\n    </div>\n    <h1 v-if=\"this.authentication\">HOLA {{ this.mail }}</h1>\n    <v-spacer></v-spacer>\n    <div class=\"busquedaProd\">\n      <v-text-field\n        color=\"white\"\n        v-model=\"text\"\n        filled\n        variant=\"solo\"\n        label=\"Buscar Producto\"\n        clearable\n      ></v-text-field>\n    </div>\n    <v-btn @click=\"buscarElementos\" icon class=\"mx-1\">\n      <v-icon>mdi-magnify</v-icon>\n    </v-btn>\n    <SearchBar />\n    <v-divider vertical></v-divider>\n    <v-menu>\n      <template v-slot:activator=\"{ props }\">\n        <v-btn icon=\"mdi-account-outline\" v-bind=\"props\"></v-btn>\n      </template>\n\n      <v-list>\n        <v-list-item\n          ><v-list-item-title v-if=\"!this.authentication\"\n            >No has iniciado sesion</v-list-item-title\n          >\n          <v-list-item-title v-else>Hola {{ this.email }}</v-list-item-title>\n        </v-list-item>\n      </v-list>\n    </v-menu>\n\n    <v-divider vertical></v-divider>\n    <v-btn icon class=\"mx-1\">\n      <v-badge color=\"#94D0EF\" content=\"2\"\n        ><v-icon>mdi-cart-outline</v-icon>\n      </v-badge>\n    </v-btn>\n    <v-menu>\n      <template v-slot:activator=\"{ props }\">\n        <v-btn icon=\"mdi-menu\" v-bind=\"props\"></v-btn>\n      </template>\n\n      <v-list>\n        <v-list-item\n          ><router-link class=\"linkRouter\" to=\"/\"\n            ><v-list-item-title>Home</v-list-item-title></router-link\n          ></v-list-item\n        >\n        <v-list-item\n          ><router-link class=\"linkRouter\" to=\"/about\"\n            ><v-list-item-title>About</v-list-item-title></router-link\n          ></v-list-item\n        >\n        <v-list-item\n          ><router-link\n            v-if=\"!this.authentication\"\n            class=\"linkRouter\"\n            to=\"/login\"\n            ><v-btn variant=\"flat\" color=\"success\">Login</v-btn></router-link\n          ><v-btn variant=\"flat\" color=\"error\" @click=\"logOut\" v-else\n            >DESCONECTAR</v-btn\n          >\n        </v-list-item>\n        <v-list-item\n          ><router-link class=\"linkRouter\" to=\"/Productos\"\n            ><v-list-item-title>Productos</v-list-item-title></router-link\n          ></v-list-item\n        >\n        <v-list-item\n          ><router-link class=\"linkRouter\" to=\"/Inicio\"\n            ><v-list-item-title>Inicio</v-list-item-title></router-link\n          ></v-list-item\n        >\n        <v-list-item\n          ><router-link class=\"linkRouter\" to=\"/Perfil\"\n            ><v-list-item-title>Perfil</v-list-item-title></router-link\n          ></v-list-item\n        >\n      </v-list>\n    </v-menu>\n  </v-app-bar>\n</template>\n\n<script>\nimport { auth, signOut } from \"@/firebaseConfig/firebaseConfig.js\";\n/*eslint-disable */\nexport default {\n  name: \"NavBar\",\n  created() {\n    debugger;\n    //OBLIGATORIO DE LO CONTRARIO NO PODEMOS TRABAJAR CON VARIABLESDFE VUE CON oAuthStateChanged\n    let self = this;\n    console.log(auth.currentUser);\n    auth.onAuthStateChanged(function (user) {\n      if (user != null) {\n        debugger;\n        console.log(\"ESTAS LOGEADO\");\n        self.email = user.email;\n        self.authentication = true;\n      } else {\n        console.log(\"NO ESTAS LOGEADO\");\n        self.authentication = false;\n      }\n    });\n    console.log(this.text);\n  },\n\n  data() {\n    return { text: \"\", searchsArray: [], authentication: null, email: \"\" };\n  },\n  methods: {\n    logOut() {\n      signOut(auth)\n        .then(() => {\n          console.log(\"Desconectado\");\n          this.authentication = false;\n        })\n        .catch((error) => {\n          console.log(\"ALGO OCURRIO\");\n        });\n    },\n    buscarElementos() {\n      this.$router.push({\n        name: \"BusquedasView\",\n        query: { prodFiltrados: JSON.stringify(this.searchsArray) },\n      });\n    },\n    async fetchAllProductsByValue(val) {\n      const data = await fetch(`http://localhost:3003/v1/api/productos`).then(\n        (res) => res.json()\n      );\n      let index = 0;\n      let encontrados = [];\n      while (index < data.length) {\n        if (\n          data[index].nombre\n            .split(\" \")\n            .join(\"\")\n            .toLowerCase()\n            .includes(val.split(\" \").join(\"\").toLowerCase())\n        ) {\n          encontrados.push(data[index]);\n        }\n        index += 1;\n      }\n      this.searchsArray = encontrados;\n    },\n  },\n  computed: {\n    authentication: false,\n  },\n  watch: {\n    text(newval, oldval) {\n      debugger;\n      this.fetchAllProductsByValue(newval);\n    },\n    authentication(newval, oldval) {},\n    inmediate: true,\n  },\n};\n</script>\n\n<style>\n.linkRouter {\n  text-decoration: none;\n  text-decoration: inherit;\n  color: inherit;\n}\n.header {\n  height: 73px;\n}\n.busquedaProd {\n  width: 20rem;\n  margin-top: 1.4rem;\n}\n.titulo {\n  display: flex;\n  position: absolute;\n  margin-left: 1rem;\n  margin-right: auto;\n  left: 0;\n  right: 0;\n  text-align: center;\n}\n\n.v-toolbar__title {\n  font-size: 1rem;\n}\n\n.v-badge__badge {\n  font-size: 10px;\n  height: 18px;\n  min-width: 18px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}