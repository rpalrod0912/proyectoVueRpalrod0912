ta = await fetch(`http://localhost:3003/v1/api/productos/ofertas`).then(res => res.json());\n      while (emptyArr.length < data.length) {\n        emptyArr.push(data[numberId]);\n        console.log(data);\n        numberId += 1;\n      }\n      return emptyArr;\n    },\n    filtrarOfertas() {\n      if (this.saleFilter) {\n        this.filterSalesArr();\n      }\n    }\n  },\n  watcher: {\n    sizesFilter(newVal, oldVal) {\n      console.log(\"Hola\");\n    }\n  }\n};","map":{"version":3,"mappings":";AA4VA,eAAe;EACb;EACA;EACAA,IAAI,EAAE,cAAc;EAEpBC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE,KAAK;MACjBC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,KAAK;MAClBC,WAAW,EAAE,KAAK;MAClBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,MAAMC,cAAc,GAAG;MACrB;;;;MAIA;MACA,IAAI,CAACL,YAAW,GAAI,MAAM,IAAI,CAACM,aAAa,EAAE;MAC9C,IAAI,CAACL,WAAU,GAAI,IAAI;MACvB;MACA,OAAO,IAAI,CAACD,YAAY;MACxBO,OAAO,CAACC,GAAG,CAAC,IAAI,CAACR,YAAY,CAAC;IAChC,CAAC;IACD,MAAMS,gBAAgB,CAACC,KAAK,EAAE;MAC5B,IAAI,IAAI,CAACP,WAAW,EAAE;QACpB,IAAI,CAACA,WAAU,GAAI,KAAK;QACxBI,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;QAClBH,OAAO,CAACC,GAAG,CAAC,IAAI,CAACN,WAAW,CAAC;MAC/B,OAAO;QACL,IAAI,CAACC,WAAU,GAAI,IAAI;QACvB,MAAMQ,KAAI,GAAI,MAAM,IAAI,CAACC,aAAa,EAAE;QACxCL,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;QAClBH,OAAO,CAACC,GAAG,CAAC,IAAI,CAACN,WAAW,CAAC;MAC/B;IACF;EACF,CAAC;EACDW,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC;IACR,CAAC;IACDC,QAAQ,EAAE;MACRF,IAAI,EAAEG;IACR,CAAC;IACDC,MAAM,EAAE;MACNJ,IAAI,EAAEG;IACR,CAAC;IACDE,MAAM,EAAE;MACNL,IAAI,EAAEG;IACR;EACF,CAAC;EACDG,OAAO,EAAE;IACPC,gBAAgB,CAACC,MAAM,EAAEC,UAAU,EAAE;MACnC,MAAMC,SAAQ,GAAIF,MAAK,GAAKA,MAAK,GAAIC,UAAU,GAAI,GAAG;MACtD,OAAOC,SAAS;IAClB,CAAC;IACD,MAAMb,aAAa,GAAG;MACpB;MACA,IAAIc,QAAO,GAAI,EAAE;MACjB,IAAIC,QAAO,GAAI,CAAC;MAChB,IAAI,CAACC,QAAO,GAAI,aAAa;MAC7B,MAAM9B,IAAG,GAAI,MAAM+B,KAAK,CACrB,gDAA8C,CAChD,CAACC,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,IAAI,EAAE,CAAC;MAC3B,OAAON,QAAQ,CAACO,MAAK,GAAInC,IAAI,CAACmC,MAAM,EAAE;QACpCP,QAAQ,CAACQ,IAAI,CAACpC,IAAI,CAAC6B,QAAQ,CAAC,CAAC;QAC7BpB,OAAO,CAA